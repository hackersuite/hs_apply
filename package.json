    "gulp-replace": "^1.0.0",
    "gulp-terser": "^1.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "source-map-support": "^0.5.13",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-mockito": "^2.4.2",
    "ts-node": "^8.3.0"
  }
