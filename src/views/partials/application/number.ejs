<div class="form-group">
  <label class="bmd-label-floating apply-label" for="<%= currentID %>">
  <%= question.questionText %>
  <%- question.required ? "<span class='label-req'> *</span>" : "" %>
  </label>
  <input
    data-hs-type="number"
    type="number" class="form-control" id="<%= currentID %>" name="<%= question.questionName %>" min="0" <%= question.required ? "required" : "" %>>
</div>